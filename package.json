{
	"name": "benchme",
	"displayName": "BenchMe",
	"description": "A Live Programming Micro Benchmarking Tool",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
		"onView:benchMe_sidebar"
	],
	"main": "./src/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "benchMe_sidebar_view",
					"title": "BenchMe",
					"icon": "assets/imgs/icon.svg"
				}
			]
		},
		"views": {
			"benchMe_sidebar_view": [
				{
					"type": "webview",
					"id": "benchMe_sidebar",
					"name": "BenchMe",
					"icon": "assets/imgs/icon.svg",
					"contextualTitle": "BenchMe"
				}
			]
		},
		"commands": [
			{
				"command": "benchme.addCase",
				"category": "BenchMe",
				"title": "Add Case"
			}
		]
	},
	"scripts": {
		"watch": "rollup -c -w",
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-node-resolve": "^11.0.1",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"concurrently": "^5.3.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"rollup": "^2.36.1",
		"rollup-plugin-css-only": "^3.1.0",
		"rollup-plugin-svelte": "^7.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.31.2",
		"svelte-check": "^1.1.26",
		"svelte-preprocess": "^4.6.1",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	},
	"dependencies": {}
}
