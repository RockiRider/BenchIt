{
	"name": "benchit",
	"displayName": "BenchIt",
	"description": "A Live Example-Based Micro Benchmarking Tool",
	"version": "0.0.1",
	"publisher": "Tsotne",
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./src/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "bench-it-sidebar-view",
					"title": "BenchIt",
					"icon": "assets/mainLogo.svg"
				}
			]
		},
		"views": {
			"bench-it-sidebar-view": [
				{
					"type": "webview",
					"id": "bench-it-sidebar",
					"name": "BenchIt"
				}
			]
		},
		"commands": [
			{
				"command": "benchit.addCase",
				"category": "BenchIt",
				"title": "Add Case"
			}
		]
	},
	"scripts": {
		"watch": "rollup -c -w",
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^17.0.0",
		"@rollup/plugin-node-resolve": "^11.0.1",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"concurrently": "^5.3.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"rollup": "^2.36.1",
		"rollup-plugin-css-only": "^3.1.0",
		"rollup-plugin-svelte": "^7.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.31.2",
		"svelte-check": "^1.1.26",
		"svelte-preprocess": "^4.6.1",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	},
	"dependencies": {
		"benchmark": "^2.1.4",
		"events": "^3.2.0",
		"express": "^4.17.1",
		"open": "^7.4.0",
		"svelte-view-engine": "^14.12.5"
	}
}
